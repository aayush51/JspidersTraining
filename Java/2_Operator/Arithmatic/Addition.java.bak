class Addition {

    public static void main(String[] args) {
        byte a = 10;
        byte b = 5;
        int answer1 = a + b;
        System.out.println(answer1);

        short c = 20;
        short d = 3;
        int answer2 = c + d;
        System.out.println(answer2);

        int e = 50;
        int f = 8;
        int answer3 = e + f;
        System.out.println(answer3);

        long g = 500;
        long h = 100;
        long answer4 = g + h;
        System.out.println(answer4);

        float i = 100.100f;
        float j = 10.10f;
        float answer5 = i + j;
        System.out.println(answer5);

        double q = 100.100;
        double r = 10.10;
        double answer6 = q + r;
        System.out.println(answer6);

        char k = 'A';
        char l = 'z';
        int answer7 = k + l;
        System.out.println(answer7);

        String m = "Aayush";
        String n = "More";
        String answer8 = m + n;
        System.out.println(answer8);

        /*
         * boolean o = true;
         * boolean p = true;
         * boolean answer9 = o + p;
         * System.out.println(answer9);
         */

    }

}
