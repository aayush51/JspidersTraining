class LCM 
{
	public static void main(String[] args) 
	{
		//LCM of Two Numbers
		int n1 = 23 , n2 = 18 ;
		int big = n1 > n2 ? n1 : n2 ;
		while (true)
		{
			if (big%n1 == 0 && big%n2 == 0)
			{
				System.out.println("LCM of " + n1 + " and " + n2 + " is : " + big);
				break;
			}
			big++;
		}
	}
}
